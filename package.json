  "license": "MIT",
  "author": "vaaski <admin@vaa.ski>",
  "scripts": {
    "dev": "concurrently npm:back:dev npm:front:dev",
    "start": "node -r dotenv/config lib/backend",
    "build": "npm run back:build && npm run front:build",
    "back:build": "rimraf lib && tsc -p tsconfig.backend.json",
    "back:dev": "nodemon",
    "front:dev": "vite --port 8080",
    "front:build": "vite build --outDir lib/dist",
    "front:serve": "vite preview",
    "commit": "cz -S"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "fastify": "^3.13.0",
    "fastify-cors": "^6.0.1",
    "fastify-static": "^4.0.1",
    "ky": "^0.28.1",
    "mongoose": "5.13.5",
    "nanoid": "^3.1.20",
    "vue": "^3.0.7"
  },
  "devDependencies": {
    "@types/mongoose": "^5.10.3",
    "@types/node": "^16.6.2",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typescript-plugin-vue": "^0.7.4",
    "autoprefixer": "^10.2.5",
    "commitizen": "^4.2.2",
    "concurrently": "^6.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "postcss": "^8.2.15",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.3",
    "vite": "^2.0.5"
  }
}
